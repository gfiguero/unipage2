{% block about %}
    {% trans_default_domain 'UniPageBundle' %}
     <section id="about">
         <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h1 class="section-header about-title">{{ 'about.title'|trans }}</h1>
                </div>
                {% for item in [1] %}
                    <div class="col-md-12 text-center">
                        <p class="about-content">{{ 'about.content'|trans }}</p>
                    </div>
                {% endfor %}
            </div>
        </div>
    </section>
{% endblock %}